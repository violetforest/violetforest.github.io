extends ./../layout.pug
block content
  div(class="content")
    div(class="content__details")
      h1.content__title GenAI Art      
  
  div(class="content")
    div(class="content__details")
      div(class="contentx__details__img")
        
        div.grid
          img(class="content__details__img content__details__img--no-padding" src="./../img/2025/holo-rose.jpg")
        
        div.grid
          video(width='70%', height='auto', playsinline, autoplay, muted, loop)
